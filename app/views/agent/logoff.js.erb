$('div#notices').empty();
<% if alert %>
show_notice('<%= alert %>', 'alert');
<% elsif @pbxis_result["response"] == "Error" %>
show_notice('<strong>Error while trying to log agent off:</strong> <%= @pbxis_result["message"] %>', 'alert');
<% else %>
toggle_logonoff_btn(<%= @queue.name %>); 
<% end %>
