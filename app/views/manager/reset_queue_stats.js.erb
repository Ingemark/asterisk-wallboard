var $queueRow = $('table#queue-statuses tr[data-queue="' +'<%= @queue_status["queue"] %>' + '"]');
        
$queueRow.find('td[data-property="abandoned"]').change_and_highlight('<%= @queue_status["abandoned"] %>', {});
$queueRow.find('td[data-property="completed"]').change_and_highlight('<%= @queue_status["completed"] %>', {});
$queueRow.find('td[data-property="holdTime"]').change_and_highlight('<%= @queue_status["holdTime"] %>', {});
$queueRow.find('td[data-property="max"]').change_and_highlight('<%= @queue_status["max"] %>', {});
$queueRow.find('td[data-property="talkTime"]').change_and_highlight('<%= @queue_status["talkTime"] %>', {});

<% @queue_status["members"].each do |extension, member| %>
    var $agentRow = $('table#agent-statuses tr[data-agent="' +'<%= extension %>' + '"]');
    $agentRow.find('td[data-property="callsTaken"]').change_and_highlight('<%= member["callsTaken"] %>', {});
    $agentRow.find('td[data-property="penalty"]').change_and_highlight('<%= member["penalty"] %>', {});
<% end %>