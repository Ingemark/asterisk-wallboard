var $queueRow = $('table#queue-statuses tr[data-queue="' +'<%= @queue_status["queue"] %>' + '"]');
        
$abandoned = $queueRow.find('td[data-property="abandoned"]');
change_and_highlight($abandoned, '<%= @queue_status["abandoned"] %>');

$completed = $queueRow.find('td[data-property="completed"]');
change_and_highlight($completed, '<%= @queue_status["completed"] %>');

$holdTime = $queueRow.find('td[data-property="holdTime"]');
change_and_highlight($holdTime, '<%= @queue_status["holdTime"] %>');

$max = $queueRow.find('td[data-property="max"]');
change_and_highlight($max, '<%= @queue_status["max"] %>');

$talkTime = $queueRow.find('td[data-property="talkTime"]');
change_and_highlight($talkTime, '<%= @queue_status["talkTime"] %>');

<% @queue_status["members"].each do |extension, member| %>
    var $agentRow = $('table#agent-statuses tr[data-agent="' +'<%= extension %>' + '"]');
    var $callsTaken = $agentRow.find('td[data-property="callsTaken"]');
    change_and_highlight($callsTaken, '<%= member["callsTaken"] %>');
    var $penalty = $agentRow.find('td[data-property="penalty"]');
    change_and_highlight($penalty, '<%= member["penalty"] %>');
<% end %>