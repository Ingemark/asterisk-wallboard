<% if @queue_statuses != nil %>
    <% @queue_statuses.each do |queue_name, status| %>
        var $queueRow = $('table#queue-statuses tr[data-queue="' +'<%= queue_name %>' + '"]');
        
        $queueRow.find('td[data-property="abandoned"]').change_and_highlight('<%= status["abandoned"] %>', {});
        $queueRow.find('td[data-property="completed"]').change_and_highlight('<%= status["completed"] %>', {});
        $queueRow.find('td[data-property="holdTime"]').change_and_highlight('<%= status["holdTime"] %>', {});
        $queueRow.find('td[data-property="talkTime"]').change_and_highlight('<%= status["talkTime"] %>', {});
        
        $queueRow = $('table#agent-statuses tr[data-queue="' +'<%= queue_name %>' + '"]');
        <% status["members"].each do |extension, member| %>
            $queueRow.find('td[data-property="callsTaken"][data-agent="' + <%= extension %> + '"]').change_and_highlight('<%= member["callsTaken"] %>', {});
    
            <% if member["lastCall"] > 0 %>
                $queueRow.find('td[data-property="lastCall"][data-agent="' + <%= extension %> + '"]').change_and_highlight('<%= Time.at(member["lastCall"]).strftime("%d.%m.%Y. %H:%M:%S") %>', {});
            <% else %>
                $queueRow.find('td[data-property="lastCall"][data-agent="' + <%= extension %> + '"]').change_and_highlight("N/A", {});
            <% end %>
        <% end %>
    <% end %>
<% end %>
